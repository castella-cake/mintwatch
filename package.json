{
    "name": "mintwatch",
    "description": "ニコニコ動画用のカスタマイズ可能な動画プレイヤー",
    "private": true,
    "version": "0.5.1",
    "type": "module",
    "license": "MIT",
    "homepage": "https://github.com/castella-cake/niconico-peppermint-extension#readme",
    "scripts": {
        "dev": "wxt",
        "dev:firefox": "wxt -b firefox",
        "build": "wxt build",
        "build:firefox": "wxt build -b firefox",
        "zip": "wxt zip",
        "zip:firefox": "wxt zip -b firefox",
        "compile": "tsc --noEmit",
        "postinstall": "patch-package && wxt prepare"
    },
    "dependencies": {
        "@dnd-kit/core": "6.3.1",
        "@dnd-kit/modifiers": "9.0.0",
        "@dnd-kit/sortable": "10.0.0",
        "@dnd-kit/utilities": "3.2.2",
        "@tabler/icons-react": "^3.12.0",
        "@xpadev-net/niconicomments": "^0.2.73",
        "dompurify": "^3.1.6",
        "hls.js": "^1.5.14",
        "html-react-parser": "5.2.2",
        "jquery": "^3.7.1",
        "jquery-ui": "^1.14.0",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-error-boundary": "5.0.0",
        "react-focus-lock": "2.13.6",
        "react-scan": "^0.2.14",
        "react-transition-group": "^4.4.5",
        "stylus": "^0.63.0"
    },
    "devDependencies": {
        "@types/chrome": "^0.0.270",
        "@types/dompurify": "^3.0.5",
        "@types/jquery": "^3.5.31",
        "@types/react": "^18.3.11",
        "@types/react-dom": "^18.3.0",
        "@types/react-transition-group": "^4.4.11",
        "@wxt-dev/module-react": "^1.1.1",
        "patch-package": "^8.0.0",
        "typescript": "^5.6.2",
        "vite": "^6.0.8",
        "wxt": "^0.19.26"
    },
    "engines": {
        "pnpm": ">=10.6"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "dtrace-provider",
            "esbuild",
            "spawn-sync"
        ]
    }
}
